<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pedido_Cliente">

    <ImageButton
        android:id="@+id/btnRegresarPedCliente"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/ListaAutos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.484"
        app:srcCompat="@drawable/regresar" />

    <TextView
        android:id="@+id/lblped5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Colchon"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.291"
        app:layout_constraintStart_toEndOf="@+id/btnLavCama"
        app:layout_constraintTop_toBottomOf="@+id/lblped4"
        app:layout_constraintVertical_bias="0.379" />

    <TextView
        android:id="@+id/lblped4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sala"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toEndOf="@+id/btnLavSillon"
        app:layout_constraintTop_toBottomOf="@+id/lblped3"
        app:layout_constraintVertical_bias="0.259" />

    <TextView
        android:id="@+id/lblped"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Realizar Pedido"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/lblpd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo de lavada"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblped"
        app:layout_constraintVertical_bias="0.071" />

    <ImageButton
        android:id="@+id/btnLavAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblpd1"
        app:layout_constraintVertical_bias="0.076"
        app:srcCompat="@drawable/pedlavauto" />

    <ImageButton
        android:id="@+id/btnLavSillon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLavAuto"
        app:layout_constraintVertical_bias="0.119"
        app:srcCompat="@drawable/pedlavsofa" />

    <ImageButton
        android:id="@+id/btnLavCama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLavSillon"
        app:layout_constraintVertical_bias="0.245"
        app:srcCompat="@drawable/pedlavcolchon" />

    <TextView
        android:id="@+id/lblped3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Automovil Tapiceria"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.525"
        app:layout_constraintStart_toEndOf="@+id/btnLavAuto"
        app:layout_constraintTop_toBottomOf="@+id/lblpd1"
        app:layout_constraintVertical_bias="0.134" />

</androidx.constraintlayout.widget.ConstraintLayout>